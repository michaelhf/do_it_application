<div class="page-header">
</div>
<div class="jumbotron text-center">
  <h1>
    Rankings
  </h1>
</div>

<div class="row">
<div class="col-xs-2"></div>
  <div class="col-xs-8">
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>User</th>
      <th>Completed</th>
      <th>Created</th>

    </tr>
  </thead>

  <tbody>
  <% @rankings.each do |ranking| %>
    <tr>
      <td><%= ranking.user.username %></td>
      <td><%= ranking.completed %></td>
      <td><%= ranking.created %></td>

    </tr>
  <% end %>
  </tbody>
</table>
</div>
</div>
